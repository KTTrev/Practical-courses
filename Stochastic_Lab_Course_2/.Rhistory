ggplot(df) +
geom_density(aes(x = rand_num, fill = method), alpha = 0.7) +
scale_colour_manual(labels = c("urban", "rural", "hi"), values = c("pink2", "green3", "blue"))
ggplot(df) +
geom_density(aes(x = rand_num, fill = method), alpha = 0.7) +
